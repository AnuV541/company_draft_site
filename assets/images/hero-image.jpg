<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 1200 600" width="1200" height="600">
  <!-- Gradient background -->
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#2c3e50;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#34495e;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <rect width="1200" height="600" fill="url(#bgGradient)"/>
  
  <!-- Industrial background pattern -->
  <circle cx="100" cy="100" r="80" fill="rgba(52, 152, 219, 0.1)"/>
  <circle cx="1100" cy="500" r="120" fill="rgba(231, 76, 60, 0.1)"/>
  
  <!-- Factory/Industrial icon -->
  <g transform="translate(900, 150)">
    <!-- Building structure -->
    <rect x="0" y="100" width="200" height="200" fill="#e74c3c" opacity="0.8"/>
    <rect x="20" y="120" width="30" height="40" fill="#3498db"/>
    <rect x="60" y="120" width="30" height="40" fill="#3498db"/>
    <rect x="100" y="120" width="30" height="40" fill="#3498db"/>
    <rect x="140" y="120" width="30" height="40" fill="#3498db"/>
    <rect x="20" y="170" width="30" height="40" fill="#3498db"/>
    <rect x="60" y="170" width="30" height="40" fill="#3498db"/>
    <rect x="100" y="170" width="30" height="40" fill="#3498db"/>
    <rect x="140" y="170" width="30" height="40" fill="#3498db"/>
    
    <!-- Chimneys -->
    <rect x="30" y="40" width="25" height="70" fill="#7f8c8d"/>
    <rect x="140" y="30" width="25" height="80" fill="#7f8c8d"/>
    <circle cx="42.5" cy="35" r="15" fill="#95a5a6" opacity="0.6"/>
    <circle cx="152.5" cy="25" r="15" fill="#95a5a6" opacity="0.6"/>
  </g>
  
  <!-- Gears -->
  <g transform="translate(150, 350)" opacity="0.6">
    <circle cx="0" cy="0" r="30" fill="none" stroke="#3498db" stroke-width="8"/>
    <rect x="-8" y="-40" width="16" height="80" fill="#3498db"/>
    <rect x="-40" y="-8" width="80" height="16" fill="#3498db"/>
    <circle cx="0" cy="0" r="15" fill="#2c3e50"/>
  </g>
  
  <!-- Text -->
  <text x="100" y="200" font-size="48" font-weight="bold" fill="#fff" font-family="Arial">
    Industrial Solutions
  </text>
  <text x="100" y="260" font-size="28" fill="#3498db" font-family="Arial">
    Premium Materials & Products
  </text>
</svg>
